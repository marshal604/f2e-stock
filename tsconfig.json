{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "paths": {
      "@shared/*": ["./src/app/shared/*"],
      "@stock/*": ["./src/app/strategy/stock/*"],
      "@core/*": ["./src/app/core/*"],
      "@dashboard/*": ["./src/app/dashboard/*"],
      "@auth/*": ["./src/app/auth/*"],
      "@strategy/*": ["./src/app/strategy/*"],
      "@gql-models/*": ["./src/gql-models/*"]
    },
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom", "esnext.asynciterable"]
  }
}
